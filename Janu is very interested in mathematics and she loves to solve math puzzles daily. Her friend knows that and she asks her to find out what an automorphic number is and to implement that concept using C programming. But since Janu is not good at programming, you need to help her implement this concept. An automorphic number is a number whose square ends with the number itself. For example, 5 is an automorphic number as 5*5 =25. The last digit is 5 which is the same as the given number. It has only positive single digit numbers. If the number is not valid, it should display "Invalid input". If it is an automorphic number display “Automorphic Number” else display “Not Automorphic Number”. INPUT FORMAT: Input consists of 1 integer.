#include<iostream>
using namespace std;
int main()
{
    int a,b,c,d;
    cout<<"enter the single digit number : ";
    cin>>a;
    if(a<1||a>9){
        cout<<"invaild number or input ";
        return 0;
    }
    
    b=a*a;
    d=b;
    c=d%10;
    if(c==a){
        cout<<"Is Automorphic Numbers ";
    }else
    {
        cout<<"Not a Automorphic Numbers ";
    }
}
