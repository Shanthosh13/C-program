#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;
    
    int sum= 0;
    int temp = n;
    while (temp>0) {
        sum+= temp % 10;
        temp /= 10;
    }
    if (n%sum==0) {
        cout << "Harshard Number" << endl;
    } else {
        cout << "Not Harshard Number" << endl;
    }
 return 0;
}
