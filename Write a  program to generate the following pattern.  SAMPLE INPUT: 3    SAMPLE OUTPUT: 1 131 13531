#include <iostream>
using namespace std;

int main() {
    int n;
    cin >> n;

    for (int i = 1; i <= n; i++) {
        // Print increasing odd numbers
        for (int j = 1; j <= 2*i - 1; j += 2) {
            cout << j;
        }
        // Print decreasing odd numbers (excluding the last)
        for (int j = 2*i - 3; j >= 1; j -= 2) {
            cout << j;
        }
        cout << endl;
    }

    return 0;
}
