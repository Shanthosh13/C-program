#include <iostream>
using namespace std;

int main() {
    string str;
   getline(cin,str);

    for (int i = 0; i < str.length(); i++) {
        if (str[i] >= 'A' && str[i] <= 'Z') {
            str[i] = str[i] - ('A' - 'a');  
        }
    }

    cout << "String with lowercase is " << str << endl;

    return 0;
}
