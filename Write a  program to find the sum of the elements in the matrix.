#include<iostream>
using namespace std;
int main(){
  int n,m;
  int sum=0;
  cin>> n >>m ;
  int arr[n][m];
  for(int i=0;i<n;i++){
    for(int j=0;j<m;j++){
      cin>> arr[i][j];
    }
  }
  for(int i=0;i<n;i++){
    for(int j=0;j<m;j++){
      sum+=arr[i][j];
    }
  }cout<<"The sum of the elements in the matrix is "<<sum;
}
