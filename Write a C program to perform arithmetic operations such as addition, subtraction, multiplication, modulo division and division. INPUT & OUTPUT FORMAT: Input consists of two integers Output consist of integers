#include <iostream>
using namespace std;
int main()
{
    int a,b;
    cout<<"enter the integer a:";
     cin>> a;
     cout<<endl;
     cout<<"enter the integer b:";
      cin>>b;
    cout<<a+b<<endl;
    cout<<a-b<<endl;
    cout<<a*b<<endl;
    cout<<a/b<<endl;
    cout<<a%b<<endl;
    
    return 0;
}
