#include <iostream>
using namespace std;

int main() {
    char str[101];
 cin.getline(str, 101);

   
    int length = 0;
    while (str[length] != '\0') {
        length++;
    }


    cout << "The reversed String is ";
    for (int i = length - 1; i >= 0; i--) {
        cout << str[i];
    }

    cout <<"."<< endl;

    return 0;
}
